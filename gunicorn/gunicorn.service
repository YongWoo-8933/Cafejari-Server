[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/srv/cafejari
ExecStart=/home/ubuntu/.local/bin/gunicorn \
        --workers 3 \
        --bind unix:/run/gunicorn.sock \
        cafejari.wsgi:application

[Install]
WantedBy=multi-user.target